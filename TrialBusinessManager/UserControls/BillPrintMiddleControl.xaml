<UserControl x:Class="TrialBusinessManager.UserControls.BillPrintMiddleControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
   Height="80"  d:DesignWidth="400">
    
    <StackPanel Orientation="Vertical"  x:Name="LayoutRoot" Background="White">
        <TextBlock Margin="7,2,40,2" HorizontalAlignment="Right"  TextWrapping="Wrap" Text="TextBlock" Name="WithOutCommTxtBlock"/>
        <TextBlock Margin="7,2,40,2" HorizontalAlignment="Right"  TextWrapping="Wrap" Text="TextBlock" Name="WithCommTxtBlock"/>
        <TextBlock Margin="30,-18,7,2" HorizontalAlignment="Left"  TextWrapping="Wrap" Text="TextBlock" Name="WarningTxtBlock" FontWeight="Bold" />
       

    </StackPanel>
</UserControl>
